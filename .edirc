# sample iirc

# important fields
nick=edirc
host=irc.freenode.net
port=6667
[[ -r ~/.password ]] && source ~/.password
# change this to your nick
owner=izabera

# all the rest
user=edirc
mode=0
realname="edirc: the ed bot"
channels=( "#edirc" )

# set it to the empty string to use ed "interactively"
trigger=:
# auths with nickserv
nickserv=yes
# check the syntax of every command issued by the owner   (helps against typos)
pedantic=yes
# log everything
log=no
timeformat=%T
# set -x
trace=no


# add extra commands here



